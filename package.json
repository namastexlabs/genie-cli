{
  "name": "@automagik/genie",
  "version": "0.260202.2234",
  "description": "Collaborative terminal toolkit for human + AI workflows",
  "type": "module",
  "bin": {
    "claudio": "./dist/claudio.js",
    "term": "./dist/term.js",
    "genie": "./dist/genie.js"
  },
  "scripts": {
    "dev:claudio": "bun --watch src/claudio.ts",
    "dev:term": "bun --watch src/term.ts",
    "version": "bun run scripts/version.ts",
    "prebuild": "bun run version",
    "build": "bun build src/claudio.ts src/term.ts src/genie.ts --outdir dist --target bun --minify && chmod +x dist/*.js",
    "build:plugin": "node scripts/build.js",
    "sync": "node scripts/sync.js",
    "build-and-sync": "npm run build:plugin && npm run sync",
    "prepack": "bun run build"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "commander": "^12.1.0",
    "zod": "^3.22.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "@types/node": "^20.10.5",
    "esbuild": "^0.27.2",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namastexlabs/genie-cli.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
